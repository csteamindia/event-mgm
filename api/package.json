{
  "name": "pg-sequelize",
  "version": "1.0.0",
  "description": "Node mySQL Sequelize boilerplate",
  "main": "./src/server.js",
  "exports": "./src/server.js",
  "scripts": {
    "start": "nodemon src/server.js",
    "test": "npm run migration && NODE_ENV=test mocha tests/*  -r dotenv/config --timeout 1000 --recursive --exit",
    "migration": "npx sequelize-cli db:migrate",
    "migration:undo": "npx sequelize-cli db:migrate:undo",
    "migration:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "coverage": "c8 --reporter lcov --reporter text --reporter html npm test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "esdoc": "esdoc",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "seed:undo:all": "npx sequelize-cli db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/csteam/node-pg-sequelize.git"
  },
  "author": "yennam111@gmail.com",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.9",
    "basic-ftp": "^5.0.5",
    "bcryptjs": "^3.0.2",
    "bwip-js": "^4.5.1",
    "chalk": "^5.3.0",
    "cls-hooked": "^4.2.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "esdoc": "^1.1.0",
    "esdoc-ecmascript-proposal-plugin": "^1.0.0",
    "esdoc-integrate-test-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "express": "^4.21.0",
    "express-async-errors": "^3.1.1",
    "express-json-validator-middleware": "^3.0.1",
    "firebase-admin": "^12.1.0",
    "helmet": "^7.1.0",
    "html-pdf-node": "^1.0.8",
    "http-status": "^1.7.3",
    "js-yaml": "^4.1.0",
    "jsbarcode": "^3.11.6",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.1",
    "node-fetch": "^3.3.2",
    "node-ssh": "^13.2.1",
    "nodemailer": "^6.10.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "radis": "^2.0.0",
    "rsa-pem-to-jwk": "^1.1.3",
    "sequelize": "^6.35.2",
    "stoppable": "^1.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "web-streams-polyfill": "^4.1.0",
    "winston": "^3.11.0",
    "xmldom": "^0.6.0"
  },
  "type": "module",
  "devDependencies": {
    "c8": "^9.1.0",
    "chai": "^5.0.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.29.1",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.4"
  },
  "c8": {
    "all": true,
    "include": [
      "src/*"
    ],
    "exclude": [
      "src/config/*",
      "src/middlewares/*",
      "src/utils/*"
    ],
    "check-coverage": false,
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
  }
}
